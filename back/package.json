{
  "name": "connect-api-4337",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && tsc && tsc-alias",
    "start": "yarn run build && node build/src/app.js ",
    "serve": "tsc-watch --onSuccess \"sh -c 'yarn run tsc-alias && node --inspect=7339 ./build/src/app.js'\"",
    "dev": "concurrently \"yarn serve\" \"yarn watch:api-specs\"",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "export NODE_ENV='test' && jest",
    "prepare": "husky install",
    "api-generate-prod": "redocly bundle ./api-specs/v1/api.yml --output dist/api.yml && npx openapi-typescript ./dist/api.yml -o ./src/customTypings/api.d.ts",
    "api-generate": "yarn api-generate-prod && eslint --fix ./src/customTypings/api.d.ts",
    "watch:api-specs": "onchange './api-specs/**/*' -- yarn api-generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometh-game/connect-api-4337"
  },
  "keywords": [],
  "author": "Cometh.io",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/cometh-game/connect-api-4337/issues"
  },
  "homepage": "https://github.com/cometh-game/connect-api-4337#readme",
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.1",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "prettier": "3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^5"
  },
  "dependencies": {
    "@redocly/cli": "^1.0.0-beta.128",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.194",
    "asn1.js": "5.4.1",
    "async": "^3.2.5",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.3",
    "bn.js": "5.2.0",
    "body-parser": "^1.20.1",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "cron": "^3.2.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.2",
    "ethers": "5.7.2",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.0.4",
